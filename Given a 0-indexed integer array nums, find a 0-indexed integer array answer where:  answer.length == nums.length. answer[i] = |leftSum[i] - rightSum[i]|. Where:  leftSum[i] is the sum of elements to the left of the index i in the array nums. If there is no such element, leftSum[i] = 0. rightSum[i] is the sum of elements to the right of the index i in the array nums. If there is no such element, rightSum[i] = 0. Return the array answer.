vector<int> leftRightDifference(vector<int>& nums) {

        vector<int>ans;
        int rsum = accumulate(nums.begin(), nums.end(), 0);
        int lsum = 0;
        for(int i = 0; i<nums.size(); i++){
            rsum-=nums[i];
            ans.push_back(abs(rsum-lsum));
            lsum+=nums[i];
        }
        return ans;
        

    }
