#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    sort(a,a+n);
	    int v=1;
	    for(int i=1;i<n;i++){
	        if(abs(a[i]-a[i-1])>1){
	            v = 0;
	            break;
	        }
	        
	    }
	    if(v==1) cout<<"YES"<<endl;
	    else cout<<"NO"<<endl;
	}
	return 0;
}
