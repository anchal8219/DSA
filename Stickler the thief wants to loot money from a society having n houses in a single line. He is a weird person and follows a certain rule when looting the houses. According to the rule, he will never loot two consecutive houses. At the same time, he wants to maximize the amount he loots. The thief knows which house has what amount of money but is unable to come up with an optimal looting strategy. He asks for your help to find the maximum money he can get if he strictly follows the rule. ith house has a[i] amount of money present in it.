  int FindMaxSum(int arr[], int n)
    {   int incl=arr[0]; 
         int excl=0; 
          int  new_excl;    
         for(int i=1;i<n;i++){
             
             new_excl=max(incl,excl); 
             
             incl=excl+arr[i]; 
             excl=new_excl;
         }
         
         return max(incl,excl);
             
        // Your code here
    }# DSA
