class Solution {
public:
    int maxProductDifference(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int ans;

        for(int i=0;i<nums.size();i++){
            int w=nums[nums.size()-1] * nums[nums.size()-2];
            int y=nums[0]*nums[1];
            ans = w-y;
        }
        return ans;
    }
};
