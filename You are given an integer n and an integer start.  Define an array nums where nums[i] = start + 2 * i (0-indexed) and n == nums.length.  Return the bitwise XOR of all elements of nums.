int xorOperation(int n, int s) {
        vector<int> nums(n);

        for (int i = 0; i < n; i++) {
            nums[i] = s + 2 * i;
        }
        int ans = nums[0];
        for(int i=1;i<n;i++){
            // nums[i] = s+2*i;
            ans = ans ^ nums[i];
        }
        return ans;
    }
