class Solution {
public:

    vector<int> getMaximumXor(vector<int>& nums, int maximumBit) {
    vector<int> v;
    int n = nums.size();
    int mask = (1 << maximumBit) - 1;  // Create a bitmask with 'maximumBit' bits set to 1

    int xorVal = 0;
    for (int i = 0; i < n; i++) {
        xorVal ^= nums[i];
    }

    for (int j = n - 1; j >= 0; j--) {
        int k = xorVal ^ mask;  // Calculate the answer for the current query
        v.push_back(k);
        xorVal ^= nums[j];      // Update XOR result by removing the last element
    }

    return v;
    }
};
