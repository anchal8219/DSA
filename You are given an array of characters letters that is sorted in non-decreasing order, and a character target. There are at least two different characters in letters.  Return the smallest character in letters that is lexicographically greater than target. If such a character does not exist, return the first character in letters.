class Solution {
public:
    char nextGreatestLetter(vector<char>& ls, char target) {
        // int l=0,h=letters.length();
        // while(l<=h){

        // }
        #include <queue>

    priority_queue<pair<int, char>, vector<pair<int, char>>,greater<pair<int, char>>> p;
    int s=target;
    for (int i = 0; i < ls.size(); i++)
    p.push({ls[i], ls[i]});

    while(!p.empty()){
        if(p.top().first>s){
            return p.top().second;
        }
        p.pop();
    }
return ls[0];
    }
};
