vector<string> summaryRanges(vector<int>& nums) {
        vector<string> str;
        int n =nums.size();
        for(int i=0;i<n;i++){
            int first = nums[i];
            while((i+1)<n && nums[i+1]==nums[i]+1)
            i++;
            if(first != nums[i])
            str.push_back(to_string(first) + "->" + to_string(nums[i]));
            else
            str.push_back(to_string(first) );
        }
        return str;
    }
