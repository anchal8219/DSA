stack<int>stk1;
    stack<int>stk2;

    MyQueue() {
        
    }
    
    void push(int x) {
        stk1.push(x);
    }
    
    int pop() {
        int v = peek();
        stk2.pop();
        return v;
        
    }
    
    int peek() {
        if(stk2.empty())
         while(!stk1.empty()){
            stk2.push(stk1.top());
            stk1.pop();
        }
        return stk2.top();
        
    }
    
    bool empty() {
        if(stk2.empty() && stk1.empty()) return true;
        else return false;
        
    }
