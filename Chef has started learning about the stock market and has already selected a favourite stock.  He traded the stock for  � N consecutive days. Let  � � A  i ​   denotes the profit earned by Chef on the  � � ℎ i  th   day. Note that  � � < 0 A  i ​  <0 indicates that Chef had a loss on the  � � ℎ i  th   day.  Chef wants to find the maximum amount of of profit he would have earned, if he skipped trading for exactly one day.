#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    
	    sort(a,a+n);
	    int sum=0;
	    for(int i=n-1;i>0;i--){
	        sum+=a[i];
	    }
	    cout<<sum<<endl;
	}
	return 0;
}
