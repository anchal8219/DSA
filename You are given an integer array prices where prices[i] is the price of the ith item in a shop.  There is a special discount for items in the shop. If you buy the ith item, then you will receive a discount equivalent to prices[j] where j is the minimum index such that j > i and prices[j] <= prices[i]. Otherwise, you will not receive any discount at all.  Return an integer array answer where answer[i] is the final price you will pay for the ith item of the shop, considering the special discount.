vector<int> finalPrices(vector<int>& prices) {
        stack<int> st;
        int n = prices.size();
        vector<int> v(n);
        v[n-1] = prices[n-1];
        st.push(prices[n-1]);
        for(int i=n-2;i>=0;i--)
        {
            while(!st.empty() && st.top()>prices[i])
            { 
                st.pop();
            }
            int mn = st.empty()==true?0:st.top();
            v[i] = prices[i]-mn;
            st.push(prices[i]);
        }
        return v;
    }
