#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,x,s,r;
	cin>>t;
	while(t--){
	    cin>>n>>x;
	    int prev_r = 0;
	    for(int i=0;i<n;i++){
	        cin>>s>>r;
	        if(s<=x && r>prev_r){
	           // cout<<r<<endl;
	            prev_r = r;
	        }
	        
	        
	    }
	    cout<<prev_r<<endl;
	    
	    
	    
	    
	}
	return 0;
}
