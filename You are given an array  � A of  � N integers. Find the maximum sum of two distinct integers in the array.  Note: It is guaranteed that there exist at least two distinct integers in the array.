#include <bits/stdc++.h>
using namespace std;

int main() {
    int t, n;
    cin >> t;
    while (t--) {
        cin >> n;
        int a[n];
        set<int> s;
        for (int i = 0; i < n; i++){
            cin >> a[i];
            s.insert(a[i]);
            
        }
        vector<int> v(s.begin(), s.end());


        cout <<v.back()+(v.at(v.size() - 2))<<endl;       
        
    }
    return 0;
}
