void permute(string s, int i,vector<string>&v){
        
        if(i==s.length()-1){
            v.push_back(s);
            return;
        }
        for(int j=i;j<s.length();j++){
            swap(s[i],s[j]);
            permute(s,i+1,v);
            swap(s[i],s[j]);
        }
        
    }
    
    vector<string> permutation(string S)
    {
        vector<string>v;
        permute(S,0,v);
        sort(v.begin(),v.end());
        return v;
    }
    
