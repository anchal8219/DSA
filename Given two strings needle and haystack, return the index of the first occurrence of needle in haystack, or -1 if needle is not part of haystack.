int strStr(string haystack, string needle) {
        int h=haystack.length(),n=needle.length();
        for(int i=0;i<h-n+1;i++){
            if(haystack[i]==needle[0]){
                if(haystack.substr(i,n+i)==needle)
                return i;
            }
        }
        return -1;
        
    }
