# DSAclass Solution {
public:
    vector<int> answerQueries(vector<int>& nums, vector<int>& qu) {
        sort(nums.begin(),nums.end());
        int sum,c;
        vector<int>v;
        for(int i=0;i<qu.size();i++){
            sum=0,c=0;
            for(int j=0;j<nums.size();j++){
                sum+=nums[j];
                if(sum<=qu[i]){
                    
                    c++;
                }
                
                else break;
            }
            v.push_back(c);
        }
        return v;
    }
};
