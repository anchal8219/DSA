#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    int c=0;
	    for(int i=0;i<n;i++){
	        for(int j=i+1;j<n;j++){
	            int nd =a[i]&a[j];
	           
	            if(nd==a[i]) c++;
	        }
	    }
	    cout<<c<<endl;
	    
	}
	return 0;
}
