# DSA
class Solution {
public:
    string reverseWords(string s) {
        int i=0,j=0;
        string ans="";
        while(i<s.length()){
            string temp="";
            while(i<s.length() && s[i]==' ') i++;
            while(i<s.length() && s[i]!=' '){
                temp += s[i];
                i++;
            } 
            if(temp!=""){
                if(ans.size()==0)
                ans = temp;
                else
                ans = temp + " " + ans;
            }
        }
        return ans;
    }
};
