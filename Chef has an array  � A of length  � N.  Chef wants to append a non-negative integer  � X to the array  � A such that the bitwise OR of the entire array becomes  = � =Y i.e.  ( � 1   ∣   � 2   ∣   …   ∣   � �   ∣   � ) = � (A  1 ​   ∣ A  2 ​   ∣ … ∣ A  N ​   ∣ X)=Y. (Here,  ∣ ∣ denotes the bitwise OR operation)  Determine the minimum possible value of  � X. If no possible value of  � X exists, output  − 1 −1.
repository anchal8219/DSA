#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,y;
	cin>>t;
	while(t--){
	    cin>>n>>y;
	    int a[n],ans=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        ans = ans|a[i];
	    }
	    if(ans==y) cout<<0<<endl;
	    else if((ans|y)!=y) cout<<-1<<endl;
	    else cout<<(ans^y)<<endl;
	    
	    
	}
	return 0;
}
# DSA
