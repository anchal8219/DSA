class Solution {
public:
    string frequencySort(string s) {
        unordered_map<char,int>m;
        priority_queue<pair<int,char>>p;

        for(int i=0;i<s.length();i++)
        {
            m[s[i]]++;
           
        }
        for(auto i=0;i<s.length();i++){
            p.push({m[s[i]],s[i]});
        }
        

        // vector<string>v;
        string f="";
        while(!p.empty()){
            f+=p.top().second;
                        // v.push_back(p.top().second);
            p.pop();
        }

        // count(s.begin(),s.end(),s[i]);
       return f;

    }
};
