int minSubArrayLen(int t, vector<int>& nums) {
        int j=0,mn=INT_MAX,sum=0;
        for(int i=0;i<nums.size();i++){
            sum += nums[i];
            while(sum>=t && j<=i){
                mn = min(mn,i-j+1);
                sum -= nums[j];
                j++;
            }
        }
        return (mn==INT_MAX) ? 0 : mn;
    }
