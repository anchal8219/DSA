# DSA
class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int p=0,mn=prices[0];
        for(int i=1;i<prices.size();i++){
            if(prices[i]<mn)
            mn = prices[i];
            else if(prices[i]>mn){
                p = max(p,prices[i]-mn);
            }
        }
        
        if(p==0) return 0;
        else return p;
    }
};
