class Solution {
public:
    string maximumOddBinaryNumber(string s) {
        int n=s.length();
        int b=0;
        for(int i=0;i<n;i++){
            if(s[i]=='1'){
                b++;
                
            }
        }
        string ans = "";
        for(int i=0;i<n;i++){
            if(b>1){
                ans += '1';
            }
            else ans += '0';
            b--;
        }
        ans.pop_back();
        return ans + '1';
        
    }
};
