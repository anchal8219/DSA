#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int n=s.length(),t=0;
	    int c=count(s.begin(),s.end(),'1');
	    int ans=0;
	    for(int i=0;i<n;i++){
	        if(s[i]=='1') t++;
	        else if(t!=0 && t!=c) {
	            ans = -1;
	            break;
	        }
	    }
	    if(c==0) ans=-1;
	    if(ans==-1) cout<<"NO"<<endl;
	    else cout<<"YES"<<endl;
	    
	}
	return 0;
}
