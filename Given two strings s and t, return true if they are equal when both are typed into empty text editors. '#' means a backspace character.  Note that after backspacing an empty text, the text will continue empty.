bool backspaceCompare(string s, string t) {
        stack<char> st1;
        stack<char> st2;
        for(auto i:s){
            if(i=='#' && !st1.empty()){
                st1.pop();
            }
            else if(i=='#' && st1.empty()) continue;
            else
            st1.push(i);
        }
        for(auto j:t){
            if(j=='#' && !st2.empty()){
                st2.pop();
            }
            else if(j!='#')
            st2.push(j);
        }

        return (st1 == st2);

    }
