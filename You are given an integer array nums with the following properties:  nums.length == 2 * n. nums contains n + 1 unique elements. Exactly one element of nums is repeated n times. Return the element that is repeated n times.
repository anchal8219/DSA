class Solution {
public:
    int repeatedNTimes(vector<int>& nums) {
        map<int,int> m;
        int n=nums.size();
        for(int i=0;i<n;i++){
            if(m[nums[i]]==1)// it means already 0 hota h
                return nums[i];
            else
                m[nums[i]]=1;
        }
        return 0;
    }
    
};
