#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    int a[n];
	    unordered_map<int,int>m;
	    int mx=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        m[a[i]]++;
	        mx = max(mx,m[a[i]]);
	    }
	    
	    
	    cout<<n-mx<<endl;
	}
	return 0;
}





