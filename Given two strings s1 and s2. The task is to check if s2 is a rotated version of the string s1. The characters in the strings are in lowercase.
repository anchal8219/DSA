class Solution
{
    public:
    bool areRotations(string s1,string s2)
    {
        if(s1.length()!=s2.length())
        return false;
        s1+=s1;
        int n=s2.length();
        
        for(int i=0;i<=n;i++){
            string ss= s1.substr(i,n);
            if(ss==s2) return true;
        }
        return false;
    }
};
