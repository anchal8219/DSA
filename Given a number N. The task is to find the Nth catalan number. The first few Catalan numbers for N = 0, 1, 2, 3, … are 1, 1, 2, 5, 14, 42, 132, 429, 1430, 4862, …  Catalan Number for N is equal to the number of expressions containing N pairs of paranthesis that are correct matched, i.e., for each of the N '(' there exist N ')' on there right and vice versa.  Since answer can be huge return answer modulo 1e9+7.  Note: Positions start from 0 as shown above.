const int mod = 1e9+7;
    int findCatalan(int n) 
    {
        //code here
        vector<int>dp(n+1);
        dp[0] = 1, dp[1] = 1;
        for(int i = 2; i <= n; i++)
        {
            int ans = 0;
            for(int j = 0; j < i; j++)
            {
                int first = dp[i-j-1]%mod;
                int second = dp[j]%mod;
                long long mul = (first*1ll*second)%mod;
                ans += mul;
                ans %= mod;
            }
            dp[i] = ans%mod;
        }
        return dp[n]%mod;
    }
