class Solution {
public:
    string largestOddNumber(string num) {
        int i=0,j=num.length()-1;
        
        while(i<=j){
            int n=num[j]-'0';
            if(n%2==0){
                j--;
            }
            else{
                string s = num.substr(i,j+1);
                return s;
            }
        }
return "";
    }
};
