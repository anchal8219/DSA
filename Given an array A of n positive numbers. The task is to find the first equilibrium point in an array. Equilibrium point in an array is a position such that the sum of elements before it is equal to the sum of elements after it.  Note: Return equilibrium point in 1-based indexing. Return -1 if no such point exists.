int equilibriumPoint(long long a[], int n) {
    
        // Your code here
        long long int r=0;
        for(int i=0;i<n;i++)
        r+=a[i];
        
        long long int l=0,d;
        for(int i=0;i<n;i++){
            r-=a[i];
            if(l==r) {
                return i+1;
