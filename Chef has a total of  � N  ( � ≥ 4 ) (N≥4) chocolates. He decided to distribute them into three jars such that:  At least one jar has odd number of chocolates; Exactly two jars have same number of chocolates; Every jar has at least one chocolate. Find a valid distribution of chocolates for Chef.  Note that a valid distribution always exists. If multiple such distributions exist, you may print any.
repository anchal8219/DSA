#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    cin>>n;
	    
	    vector<int>v;
	    if(n%2==0){
	            v.push_back(1);
	            v.push_back(1);
	            v.push_back(n-2);
	    }
	    else{
	        v.push_back(1);
	        n=n-1;
	        v.push_back(n/2);
	        v.push_back(n/2);
	    }
	    for (int i=0;i<v.size();i++) {
            cout << v[i]<<" ";
        }
        cout << endl;
	}
	return 0;
}
