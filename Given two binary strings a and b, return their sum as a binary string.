string addBinary(string a, string b) {
        int al = a.length();
        int bl = b.length();
        int i=0,carry=0;
        string ans="";
        while(i<al || i<bl || carry!=0){
            int x=0;
            if(i<al && a[al - i -1]=='1')
            x=1;

            int y=0;
            if(i<bl && b[bl - i -1]=='1')
            y=1;

            ans=to_string((x+y+carry)%2) + ans;
            carry = (x+y+carry)/2;
            i+=1;
        }
        return ans;

        
    }
