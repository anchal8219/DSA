class Solution {
public:
    int findKOr(vector<int>& nums, int k) {
        int res=0;
        for(int i=0;i<32;i++){
            int c=0;
            for(int j=0;j<nums.size();j++){
                if(nums[j]&(1<<i)){
                    // nums[j]>>=1;
                    c++;
                    if(c==k){
                        res += pow(2,i);
                        break;
                    }
                }
            }
        }
        return res;
    }
};
