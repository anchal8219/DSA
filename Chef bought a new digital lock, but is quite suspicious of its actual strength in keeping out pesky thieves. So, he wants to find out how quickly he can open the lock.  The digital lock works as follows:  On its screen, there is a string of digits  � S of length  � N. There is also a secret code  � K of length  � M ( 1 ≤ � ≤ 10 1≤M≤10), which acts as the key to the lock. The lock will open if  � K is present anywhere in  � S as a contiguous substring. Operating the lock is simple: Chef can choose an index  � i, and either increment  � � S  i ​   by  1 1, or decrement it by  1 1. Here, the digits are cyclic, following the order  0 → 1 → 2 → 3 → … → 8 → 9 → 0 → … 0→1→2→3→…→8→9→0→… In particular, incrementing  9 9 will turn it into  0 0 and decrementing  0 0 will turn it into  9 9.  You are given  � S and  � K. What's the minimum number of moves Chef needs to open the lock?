#include<iostream>
#include<climits>
using namespace std;

int main() {
    int t;
    cin>>t;
    
    while (t--) {
        int n,m;
        cin>>n>>m;
        
        string s,k;
        cin>>s>>k;
        
        int res=INT_MAX;
        
        for (int i=0;i<=n-m;i++) {
            int c=0;
            for (int j=0;j<m;j++) {
                int d=abs((int)s[i+j]-(int)k[j]);
                c+=min(d,10-d);
            }
            res=min(res,c);
        }
        cout<<res<<endl;
    }
    
    
    return 0;
}
