string decodeMessage(string key, string ms) {
        map<char, char> m;
        char ch='a';

        for(int i=0;i<key.size();i++){
            if(m.find(key[i])== m.end() && key[i]!=' ')
            m[key[i]]= ch++;
            // ch++;
        }
        string d = "";
        for(auto i:ms){
            if(i!=' '){
                d += m[i];
            }
            else 
            d += ' ';
        }
        return d;
    }
