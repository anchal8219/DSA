double findMaxAverage(vector<int>& nums, int k) {
        double sum=0;
        for(int i=0;i<k;i++)
        sum += nums[i];

        double avr = sum/k;

        for(int i=k;i<nums.size();i++){
            sum = sum + nums[i]-nums[i-k];
            double av = sum/k;
            avr = max(avr,av);
        }

return avr;
    }
