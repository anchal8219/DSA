class Solution {
public:
void combo(int i,int t,vector<int> &a, vector<vector<int>>&ans, vector<int>&ds){
    if(i==a.size()){
        if(t==0){
            ans.push_back(ds);
        }
        return;
    }
    //pick the element
    if(a[i]<=t){
        ds.push_back(a[i]);
        combo(i,t-a[i],a,ans,ds);
        ds.pop_back();

    }
    combo(i+1,t,a,ans,ds);
}
    vector<vector<int>> combinationSum(vector<int>& candidates, int t) {
        vector<vector<int>>ans;
        vector<int>ds;

        combo(0,t,candidates,ans,ds);
        return ans;
    }
};
