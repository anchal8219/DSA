#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k;
	    cin>>n>>k;
	    int a[n],sum=0;
	    for(int i=0;i<n;i++){
	    cin>>a[i];
	    sum+=a[i];
	    }
	    if(sum<n) cout<<"no"<<endl;
	    else if(sum%n==0){
	        cout<<"yes"<<endl;
	    }
	    else if(k>0) cout<<"yes"<<endl;
	    else cout<<"no"<<endl;
	
	    
	}
	return 0;
}
