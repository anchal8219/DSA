#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t,n;
	cin>>t;
	while(t--){
	    
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    cin>>a[i];
	    int c=0,mx=0;
	    for(int i=0;i<n;i++){
	        if(a[i]>mx){
	            mx=a[i];
	        }
	        else c++;
	    }
	    
	  cout<<c<<endl;

	    
	}

}
