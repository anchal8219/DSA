class Solution {
public:
    int minOperations(vector<int>& nums) {
        int n=nums.size(),ans=0;
        unordered_map<int,int>m;
        for(int i=0;i<n;i++){
            m[nums[i]]++;            
        }
        for(auto j:m){
            if( j.second==1) return -1;
            
            if(j.second % 3 > 0) ans += 1;
            ans += j.second/3;
        }
        return ans;
    }
};
