vector<int> singleNumber(vector<int> nums) 
    {
        // Code here.
        map<int,int>m;
        vector<int>v;
        for(int i=0;i<nums.size();i++){
            m[nums[i]]++;
            
        }
        for(int i=0;i<nums.size();i++){
            if(m[nums[i]]==1)
            v.push_back(nums[i]);
        }
        sort(v.begin(),v.end());
        return v;
    }
