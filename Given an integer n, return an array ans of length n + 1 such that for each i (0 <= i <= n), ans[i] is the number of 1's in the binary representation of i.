vector<int> countBits(int n) {
        int c;
        vector<int>ans;
        for(int i=0;i<=n;i++){
            c=0;
            int num=i;
            while(num!=0){
                if((num&1)!=0){
                    c++;
                }
                num=num>>1;
            }
            ans.push_back(c);
        }
        return ans;
    }
