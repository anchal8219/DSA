class MinStack {
public:
    stack<int> stk;
    int m = INT_MAX;
    MinStack() {
    }
    
    void push(int x) {
        if(x <= m){ 
            stk.push(m);
            m = x;
        }
        stk.push(x);
    }
    
    void pop() {
        int tmp = stk.top();
        stk.pop();
        if(tmp == m){
            m = stk.top();
            stk.pop();
        }
    }
    
    int top() {
        return stk.top();
    }
    
    int getMin() {
        return m;
    }
};
