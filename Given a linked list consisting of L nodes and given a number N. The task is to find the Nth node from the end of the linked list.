# DSAint getNthFromLast(Node *head, int n)
    {
        Node* ptr1 = head;
        Node* ptr2 = head;
        
        // offset ptr2 by n
        for(int i = 1; i < n; i++){
            if(ptr2 -> next){
                ptr2 = ptr2 -> next;
            } else {
                return -1;
            }
        }
        
        while(ptr2 -> next){
            ptr1 = ptr1 -> next;
            ptr2 = ptr2 -> next;
        }
        return ptr1 -> data;
    }
