int lengthOfLastWord(string s) {
        int c=0,n=s.length();
        int flag=false;
        for(int i=n-1;i>=0;i--){
            if(s[i]!= ' '){
                flag=true;
                c++;
            }
                
            else if(s[i]== ' ' && flag== true){
                break;
            }
        }
        return c;
    }
